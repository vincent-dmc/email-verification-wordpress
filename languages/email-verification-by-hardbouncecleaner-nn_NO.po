msgstr "Url"

msgid "Unable to create directory"
msgstr "Kan ikke opprette katalog"

msgid "Database update version %d error"
msgstr "Database oppdateringsversjon% d feil"

msgid "An exception occured"
msgstr "Et unntak oppstod"

msgid "yes"
msgstr "ja"

msgid "error"
msgstr "feil"

